y1 <- CD4Data$Promct
plot(x1, y1)
CD4Amodel <- lm(y1~x1)
CD4Amodel_summary <- summary(CD4Amodel)
slopeCD4value <- CD4Amodel_summary$coefficients[2,1]
# Pendiente de la curva
slopeCD4value
# Eficiencia del cebador en porcentaje
(10^(-1/slopeCD4value)-1)*100
PlotCD4
